{
  "name": "proto-x",
  "displayName": "Proto X",
  "description": "Protobuf extension.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:proto-x.helloWorld",
    "onLanguage:proto-x"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "proto-x",
        "aliases": [
          "Proto X",
          "proto-x"
        ],
        "extensions": [
          ".protox"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "proto-x",
        "scopeName": "source.protox",
        "path": "./syntaxes/proto-x.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "proto-x.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "vscode-test": "^1.6.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  },
  "packageManager": "yarn@3.1.1"
}
