{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Proto X",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.proto-x",
          "match": "\\b(message|syntax|option|package)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.proto-x",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.proto-x",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.protox"
}
